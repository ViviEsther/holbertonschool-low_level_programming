#include "main.h"

/** <>
* print_times_table - function that prints the n times table
*
* Return: void.
*/

void print_times_table(int n)
{
	int n;
	int i;
	int j;

	if (n > 0 && n <= 15)
	{
		for (i = 0; i <= n; i++)
		{
			for (j = 0; j <= n; j++)
			{
				_putchar(i + '0');
			}
		}

	}
	else
	{

	}



}
